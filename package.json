{
    "name": "react-login-form",
    "version": "1.0.0",
    "description": "React login form for node.js",
    "main": "index.js",
    "scripts": {
        "dev": "babel --watch src/js/index.jsx --out-file index.js --source-maps",
        "test": "jest",
        "watch:test": "nodemon -w __tests__ -w ./src -e js,jsx -x npm test"
    },
    "author": "Kristoffer Hebert",
    "license": "ISC",
    "dependencies": {
        "react": "^0.13.3"
    },
    "devDependencies": {
        "babel-jest": "^5.3.0",
        "jest-cli": "^0.5.5"
    },
    "jest": {
        "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
        "unmockedModulePathPatterns": ["<rootDir>/node_modules"],
        "testFileExtensions": [
            "jsx",
            "js"
        ],
        "moduleFileExtensions": [
            "jsx",
            "js"
        ]
    }
}
